{% extends "base.html.twig" %}
{%block title %}
    {{parent()}} - Liste des systèmes
{%endblock%}
{%block body %}
<h1>Liste des systèmes</h1>

<table>


    <tr>
        <th>Nom du système</th>
        <th></th>
        <th>URL</th>
    </tr>

    {% for system in systemListe %}


        <tr>
            <td>{{ system.nom}}</td>
            <td></td>
            <td>{{ system.url }}</td>
            <td><a href="{{ path('system_edit', {'id' : system.id}) }}">Modification du système</a></td>
        </tr>

    {% endfor %}

</table>

<a href="{{ path('system') }}">Retour</a>
{%endblock%}
